package chap04;

public class ifExam {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		
		int score = 93;
		
		if(score>=90) {
		  System.out.println("점수가 90보다 큽니다");
		  System.out.println("등급은 A입니다");
		
		}else if(score>=97) {
			System.out.println("점수가 80");
			//else가 있다는 건 무조건 하나의 실행 블럭이 실행 된다.
		}
		
	     
		
		
	     
	     
		
	
	}	
	

}
