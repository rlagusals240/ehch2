		  System.out.println("점수가 90보다 큽니다");
		  System.out.println("등급은 A입니다");
		
		}else if(score>=91) {
			System.out.println("점수가 90");
			//else가 있다는 건 무조건 하나의 실행 블럭이 실행 된다.
		}
	//	if와 관계 없이 무조건 판단한다.
	     
	          int x 3;
	          (if x%2 ==0);
	 {
		 System.out.println("짝수");
		 //else if( x%2 != 0) 
