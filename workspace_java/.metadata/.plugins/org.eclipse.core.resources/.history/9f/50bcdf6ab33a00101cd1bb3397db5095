package sec01.exam01;

public class VarTypeExam {

	public static void main(String[] args) {
		
	  byte b1;
	  b1 = 127;
	  System.out.println("b1: "+ b1);
	  //b1 = 128; 바이트가 127이 마지막단어라서 이건안됨
/////////////////////////////////////////////////////////////////////////
		
	  char c1 = 65;
	  System.out.println("c1: "+ c1);
	  c1 = 65 + 2;
	  System.out.println("c1: "+c1);
	  //char는 a는 65이기때문에 글씨전용이라서 a가 나옴
		
	  char c2 = 'F'; //char type에만 한글자만 들어갈 경우 홀따움표로 감싸준다; 두 글자는 안된다.
	 System.out.println("c2: "+ c2);
	 System.out.println("c1-c2:" + (c2-c1));
	 
	 char c3 = '한';
	 System.out.println("c3:" +c3);
	 System.out.println("c3:" +(c3+1));
///////////////////////////////////////////////////////////	 
	 //string은 s를 대문자로해야함
	 String s1 = "김현민";
	 System.out.println("s1:" +s1);
	 
	 String s2 = "김\"현민";    //String s2 = "김"현민";슬래쉬 안붙일경우 실행 안됨 \역슬래쉬 (\n이 한글자임)
     System.out.println("s2:" +s2);
     
     String s3 = "김\t현\n민";    //String s2 = "김"현민";슬래쉬 안붙일경우 실행 안됨 \역슬래쉬
     System.out.println("s3:" +s3);
     
     s3 = s3 + "글씨" + 3 + 2 + (3 + 2);
     System.out.println(s3);
     String s4 = 3 + 2 + "글씨";
     System.out.println(s4);
     
    // String s5 = s3 + s4;
     
    // int i = 2100000000; 이건 되고
     //int i = 2200000000 이건 안된다
     // L또는 L을 붙이면 명시적인 Long
     //안붙이면 무조건 int로 생각한다.
     //long l1 = 2100000000L;
	 //long l2 = 123;
     float f1 = 3.14f;
     System.out.println("f1:"+ f1 );
    double d1 = 3.14d;
    System.out.println("d1:"+ d1 );
	
    //float의 정밀도 : 소수점 7자리
    //소수점 7자리숫자까지만 가능함 그 뒤로는 다 반올림함
    float f2 = 0.12345678f;
    System.out.println("f2:" +f2);
    
    //double의 정밀도 : 소수점 16자리
    double d2 = 0.12345678901234567890;
    System.out.println("d2: "+d2);
	
	double i10 = 5e2; 
	
    boolean stop = true;
    boolean start = false;
	
    //논리타입
    //boolean 
    //1byte
    //true, false 값을 넣을 수 있다.
	
    
   // 1, 내 나이를 저장하시오 (age)
     //int age = 20;  
    
    // 2, 소유 차가 있다 (car)
	//boolean car = true;
   //3 우리집에 있는 스마트폰의 개수
      //nt phone = 3
     
  // 4 내 이름
     //String name = "김현민";
     
  // 5 1평은 3.3제곱미터입니다 5평은   
	//double area = 5 * 3.3;
	
	// 퀴즈 0
	/*
	 * 두 변수 a,b에 각각 3,4를 넣고
	 * 출력 "3과 4". 단 ,변수를 활용해서
	 */
	 // int a = 3;
	// int b = 4;
	 // System.out.println(a +" 과 "+ b);
	
	  /*1-1 두 변수 a, b에 각각 3, 4를 넣고
	   * 출력 결과 :"3 > 4의 결과는 false입니다
	   *
	   *
	   * 1-2 : 두 변수의 값을 6,5 변경
	   * 출력 결과 : "6 > 5의 결과는 true 입니다
	   * 퀴즈 2 숫자 132 출력 결과 
	   * 백의 자리 : 1"
	   * 십의 자리 : 3"
	   * 일의 자리 : 2"
	   */
	
	   int a = 3;
	   int b = 4;
	   System.out.println(a+"과 " +b);
			
	
	
	
	
	
	
	}

}



